pipeline{
    agent any
    
    stages{
        stage('Build'){
            steps{
                sh 'echo "build...DEV"'
            }
        }
        stage('Test'){
            steps{
                sh 'echo "Je teste...DEV"'
            }
        }
        stage('Deploy'){
            steps{
                sh 'echo "Je deploy...DEV"'
            }
        }
    }
}