pipeline{
    agent any
    
    stages{
        stage('Build'){
            steps{
                sh 'echo "build...feature 01"'
            }
        }
        stage('Test'){
            steps{
                sh 'echo "Je teste...feature 01"'
            }
        }
        stage('Deploy'){
            steps{
                sh 'echo "Je deploy...feature 01"'
            }
        }
    }
}